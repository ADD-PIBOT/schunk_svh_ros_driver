<launch>
  <arg name="trajectory_controller_name" />

  <!-- Load controller settings -->
  <rosparam file="$(find schunk_svh_driver)/cfg/svh_controllers.yaml" command="load"/>

  <!-- spawn controller manager (with a timeout of 55 to give enough time to finger reset)-->
  <node name="hand_ros_control_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="--timeout 55 $(arg trajectory_controller_name)" />

  <!-- load publishing controller -->
  <node name="hand_ros_control_controller_manager" pkg="controller_manager" type="controller_manager" respawn="false"
    output="screen" args="load joint_state_controller" />

</launch>
